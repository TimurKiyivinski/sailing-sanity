<link rel="stylesheet" href="/styles/app.css">
<div class="container">
    <div class="row">
        <div class="page-header">
            <h2 id="scannerheader">Scanners <small><i class="fa fa-spinner fa-spin"></i></small></h2>
        </div>
        <div class="col-xs-12 col-md-6">
            <select id="scanners" class="form-control" data-url="<%= sails.getBaseurl() + '/scanner/get' %>">
            </select>
        </div>
        <div class="col-xs-12 col-md-6">
            <a id="scan" class="btn btn-danger col-xs-12" type="button" data-url="<%= sails.getBaseurl() + '/scanner/scan' %>">Scan</a>
        </div>
    </div>
    <div class="row">
        <div class="page-header">
            <h2 id="scanheader">Scans</h2>
        </div>
        <div class="grid" data-url="<%= sails.getBaseurl() + '/scan/' %>" data-create="<%= sails.getBaseurl() + '/scan/create' %>">
        </div>
    </div>
</div>
<div id="template" class="col-xs-12 col-md-4 grid-item hidden">
    <a class="img-delete btn btn-danger">
        <i class="fa fa-close"></i>
    </a>
</div>
<script type="text/javascript" src="js/app.js"></script>
